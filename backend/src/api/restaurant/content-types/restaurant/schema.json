{
  "kind": "collectionType",
  "collectionName": "restaurants",
  "info": {
    "singularName": "restaurant",
    "pluralName": "restaurants",
    "displayName": "Restaurant",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string",
      "unique": true
    },
    "Description": {
      "type": "blocks"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "mappedBy": "restaurants"
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "restaurant"
    },
    "reservation": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::reservation.reservation",
      "mappedBy": "restaurant"
    },
    "Media": {
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ],
      "type": "media",
      "multiple": true
    },
    "newsletter_subscribers": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::newsletter-subscriber.newsletter-subscriber",
      "mappedBy": "restaurants"
    }
  }
}
